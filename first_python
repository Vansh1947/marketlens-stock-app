a= input("enter the number1"  ) 
b= input("enter the number2"  )
print("number1 is", a)
print("number2 is", b)
print("sum is", int(a)+int(b))
print("difference is", int(a)-int(b))
print("product is", int(a)*int(b))
print("division is", int(a)/int(b))
print("modulus is", int(a)%int(b))